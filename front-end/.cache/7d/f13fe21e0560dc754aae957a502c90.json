(b.votes - parseFloat(a.votes)));\n};\n\n// rerenders new list on vote\nmakeList = () => {\n  listGroup.empty();\n  sortList();\n  orderedList.forEach(item => {\n    listGroup.append(`\n    <li\n    class=\"list-group-item d-flex justify-content-between align-items-center\"\n  >\n    <span>\n      <span class=\"badge badge-primary badge-pill\">${item.votes}</span>\n      ${item.name} \n      </span>\n    </span>\n    <span>\n      <button\n        onclick=\"{handleUpVote(event)}\"\n        type=\"button\"\n        class=\"btn btn-primary\"\n      >\n        +\n      </button>\n      <button\n        onclick=\"{handleDownVote(event)}\"\n        type=\"button\"\n        class=\"btn btn-primary\"\n      >\n        -\n      </button>\n    </span>\n   </li>\n    `);\n  });\n};\n\nhandleUpVote = e => {\n  const text = e.target.closest(\"li\").innerText;\n  list.forEach(item => {\n    if (text.includes(item.name)) {\n      item.votes++;\n    }\n  });\n  makeList();\n};\n\nhandleDownVote = e => {\n  const text = e.target.closest(\"li\").innerText;\n  list.forEach(item => {\n    if (text.includes(item.name)) {\n      item.votes--;\n    }\n  });\n  makeList();\n};\n\n$(document).ready(() => {\n  makeList();\n});\n"},"lineCount":null}},"error":null,"hash":"691253bda109d558bd108aeebbc92502","cacheData":{"env":{}}}